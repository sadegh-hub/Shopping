@model IEnumerable<Shapping.Models.OrdereDetail>

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_User_Layout.cshtml";
}

<h2>Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Order.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Productkala.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quntity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Order.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Productkala.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quntity)
        </td>
        
    </tr>
}

</table>
